<%= form_with url: import_telephone_book_path(@telephone_book), local: true, method: :post do |form| %>
  <%= render 'shared/errors', resource: @telephone_book %>
  <div class="form-group mt-3">
    <p>
      <%= form.label :file %>
      <%= form.file_field :file, class: 'form-control-file', required: true%>
    </p>
    <%= form.submit 'Upload contacts', class: 'btn btn-success '%>
  </div>
<% end %>
